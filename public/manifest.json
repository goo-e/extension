{
  "manifest_version": 2,
  "short_name": "goophur",
  "name": "goo e. goophur",
  "description": "build better queries with this google advanced search gui",
  "version": "0.0.4",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "goophur"
},
"icons": {
  "16": "images/16.png",
  "48": "images/48.png",
  "128": "images/128.png"

},
"content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'",
"permissions": [
  "*://goophur.herokuapp.com/*",
  "activeTab",
  "cookies",
  "storage"
],
"background": {
  "scripts": ["background.js"],
  "persistent": false
}
}